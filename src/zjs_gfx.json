{
    "module": "gfx",
    "require": "gfx",
    "depends": ["buffer", "fs"],
    "zephyr_conf": {
        "all": [
            "CONFIG_REBOOT=y"
        ]
    },
    "zjs_config": [
        "-I${ZEPHYR_BASE}/ext/fs/fat/include",
        "-I./deps",
        "-DBUILD_MODULE_GFX",
        "-DZJS_DYNAMIC_LOAD"
    ],
    "src": [
        "src/zjs_gfx.c",
        "src/ashell/file-utils.c"
    ]
}
